/**index.wxss**/
page {
  height: 100%;
  background: #fff
    url('https://cmspic-10004025.image.myqcloud.com/b37f43e0-6937-11e8-9625-95277b0262f6_size_750x1334')
    no-repeat top center;
  background-size: cover;
}
.container {
  height: 100%;
  padding: 0 25rpx;
  box-sizing: border-box;
}

.header {
  margin: 50rpx 0;
  overflow: hidden;
  float: left;
  font-size: 60rpx;
  color: #000;
}

.field {
  display: -webkit-box;
  display: -webkit-flex;
  display: -moz-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  -moz-box-pack: justify;
  -ms-flex-pack: justify;
  justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
  -moz-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  width: 100%;
  height: 112rpx;
  line-height: 112rpx;
  border-bottom: 1rpx solid #e6e6e6;
  font-size: 30rpx;
  color: #444;
}

.codeButton {
  margin: 0;
  padding: 0;
  -webkit-box-align: center;
  -webkit-align-items: center;
  -moz-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  width: 160rpx;
  height: 60rpx;
  line-height: 60rpx;
  color: #fff;
  font-size: 24rpx;
  background: #ff397e;
}

.countdown {
  width: 160rpx;
  height: 60rpx;
  line-height: 60rpx;
  color: #fff;
  font-size: 24rpx;
  border-radius: 0;
  background: #ccc;
  text-align: center;
}

.clear {
  display: -webkit-box;
  display: -webkit-flex;
  display: -moz-box;
  display: -ms-flexbox;
  display: flex;
}

.agreement {
  margin-top: 20rpx;
  font-size: 24rpx;
  line-height: 1.5;
  color: #c7c7cd;
  text-align: center;
}

.agreement .link {
  display: inline;
  color: #ff397e;
}

.button {
  margin-top: 50rpx;
  width: 100%;
  height: 100rpx;
  background: #ff397e;
  color: #fff;
  font-size: 36rpx;
}
.reg-guide .logo {
  display: flex;
  justify-content: center;
  margin: 0 0 58.4rpx;
}
.reg-guide .logo .img {
  width: 328rpx;
  height: 50rpx;
  display: block;
}
.reg-guide {
  padding: 0 50rpx;
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  box-sizing: border-box;
}
.reg-guide .agreement-text {
  margin-top: 24rpx;
  margin-bottom: 80.4rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 24rpx;
  font-size: 24rpx; 
}
.reg-guide .agreement-text .normal-text {
  color: #666;
}
.reg-guide .agreement-text .highlight-text {
  color: #FF6198;
}
.reg-method .btn {
  margin: 0;
  font-size: 32rpx;
  line-height: 96rpx;
  height: 96rpx;
  border-radius: 8rpx;
}
.reg-method .weixin {
  background: #01be0b;
  color: #fff;
}
.reg-method .btn:after {
  border: none;
}
.reg-method .weixin:before {
  content: '';
  background: url('data:image/png;base64,')
    no-repeat center;
  display: inline-block;
  width: 44rpx;
  height: 44rpx;
  background-size: contain;
  vertical-align: middle;
  margin-right: 20rpx;
}
.reg-method .phone {
  color: #000;
  background: #fbfafc;
}
.reg-method .phone:after {
  color: #000;
  border: 1px solid #000;
}
