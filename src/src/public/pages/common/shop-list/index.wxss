.title {
  display: flex;
  height: 100rpx;
  line-height: 100rpx;
  background: #fff;
  padding: 0 25rpx;
  border: 1rpx solid #e7e7e7;
  border-width: 1rpx 0;
  margin-bottom: 5rpx;
}
.title .label {
  flex: 1;
  font-size: 32rpx;
  font-weight: bold;
}
.selector {
  font-size: 28rpx;
  height: 60rpx;
  background: #fff;
  border-radius: 60rpx;
  white-space: nowrap;
  position: absolute;
  top: 20rpx;
  left: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 0 55rpx 0 25rpx;
  color: #444;
  box-shadow: 0 3px 4px rgba(0, 0, 0, 0.24);
}
.selector:after {
  content: '';
  display: block;
  position: absolute;
  right: 25rpx;
  top: 18rpx;
  margin-left: 14rpx;
  border: 2px solid #444;
  border-width: 2px 2px 0 0;
  width: 12rpx;
  height: 12rpx;
  transform: rotate(135deg);
}
.store {
  margin-bottom: 10rpx;
  padding: 30rpx 25rpx 0;
  background-color: #fff;
  align-items: flex-start;
}
.store .feature {
  display: flex;
  align-items: flex-start;
}
.store .hd {
  font-size: 32rpx;
  line-height: 1.5em;
  margin-bottom: 10rpx;
  display: flex;
}
.store .hd view:nth-child(1) {
  flex: 1;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  flex-basis: 100px;
  flex-grow: 1;
  flex-shrink: 1;
}
.store .hd view:nth-child(2) {
  color: #666;
  font-size: 24rpx;
}
.store .hd view:nth-child(2):before {
  content: '';
  display: inline-block;
  margin-right: 8rpx;
  width: 14rpx;
  height: 18rpx;
  background: url('data:image/png;base64,');
  background-size: contain;
}
.store .addr,
.store .phone {
  color: #999;
  font-size: 24rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  line-height: 1.8em;
}
.store .pic {
  margin-right: 20rpx;
}
.store .pic image {
  width: 200rpx;
  height: 150rpx;
  display: block;
}
.store .info {
  flex-grow: 1;
  flex-shrink: 1;
  flex-basis: 200rpx;
  overflow: hidden;
}
.store .svr {
  white-space: nowrap;
  overflow: auto;
  padding: 18rpx 0;
  border-bottom: 1px dashed #e8e8e8;
}
.store .svr .item {
  display: inline;
  margin-right: 24rpx;
}
.store .svr image {
  width: 32rpx;
  height: 32rpx;
  vertical-align: middle;
  margin-right: 8rpx;
}
.store .svr text {
  vertical-align: middle;
  font-size: 20rpx;
  color: #999;
}
.store .tool {
  display: flex;
}
.store .tool .btn {
  flex: 1;
  font-size: 28rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 30rpx 0;
}
.store .tool .btn:before {
  content: '';
  display: block;
  width: 32rpx;
  height: 32rpx;
  margin-right: 8rpx;
  background: no-repeat center;
  background-size: contain;
}
.store .tool .call::before {
  background-image: url('data:image/png;base64,');
}
.store .tool .navigate::before {
  background-image: url('data:image/png;base64,');
}
.store .tool .detail::before {
  background-image: url('data:image/png;base64,');
}
.store.more .info {
  padding-right: 20rpx;
  position: relative;
}
.store.more .info:after {
  content: '';
  display: block;
  border: 2px solid #e7e7e7;
  border-width: 2px 2px 0 0;
  width: 20rpx;
  height: 20rpx;
  position: absolute;
  right: 0;
  top: 48%;
  transform: rotate(45deg) skew(15deg, 15deg);
}
.empty {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-top: 100rpx;
}
.empty .icon {
  width: 200rpx;
  height: 200rpx;
}
.empty text {
  color: #666;
  font-size: 28rpx;
  margin: 30rpx 0 60rpx;
}
.empty .btn {
  display: block;
  width: 250rpx;
  height: 80rpx;
  background-color: #ff6ea2;
  color: #fff;
  text-align: center;
  line-height: 80rpx;
  font-size: 32rpx;
}
page {
  height: 100%;
}
.shop-list {
  height: 100%;
  display: flex;
  flex-direction: column;
}
.map {
  flex: 1;
  width: 100%;
  position: relative;
}
.list {
  flex-shrink: 1;
  overflow: auto;
}
.list.multi {
  height: 650rpx;
}
.city-selector {
  position: absolute;
  top: -200rpx;
  left: -200rpx;
  /* left: 0;
  top: 0;*/
}
.single-store .store {
  margin-bottom: 0;
}
.touchbar {
  height: 52rpx;
  background: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  border-bottom: 1px solid #e7e7e7;
}
.touchbar:after {
  content: '';
  display: block;
  width: 90rpx;
  height: 10rpx;
  background: #e7e7e7;
  border-radius: 10rpx;
}
