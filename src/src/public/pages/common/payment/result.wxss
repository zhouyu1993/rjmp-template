Page {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 50%;
}
view {
  font-size: 32rpx;
  margin-top: 30rpx;
}
.btns {
  display: flex;
}
.btns navigator {
  background: #ff397e;
  color: #fff;
  font-size: 38rpx;
  border-radius: 10rpx;
  display: block;
  margin: 20rpx;
  width: 300rpx;
  box-sizing: border-box;
  text-align: center;
  line-height: 88rpx;
}
.btns navigator:first-child {
  border: 1px solid #ff397e;
  background: #fff;
  color: #333;
}
