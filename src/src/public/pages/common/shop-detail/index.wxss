.block {
  background-color: #fff;
  margin: 20rpx 0;
}
.block .title {
  font-size: 30rpx;
  border-bottom: 1px solid #e7e7e7;
  padding: 20rpx 0;
  margin-left: 20rpx;
}
.base {
  overflow: hidden;
  padding-left: 20rpx;
}
.base .name {
  font-size: 36rpx;
  margin: 30rpx 0;
  line-height: 42rpx;
  vertical-align: middle;
}
.base .name::before {
  content: "";
  display: inline-block;
  width: 42rpx;
  height: 42rpx;
  background: url("data:image/png;base64,")
    no-repeat center;
  vertical-align: middle;
  background-size: contain;
  margin-right: 18rpx;
}
.info {
  font-size: 28rpx;
  display: flex;
  align-items: center;
  margin: 20rpx 0;
}
.info .txt {
  flex: 1;
  flex-basis: 300rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.info .time {
  display: flex;
}
.info .time text {
  color: #666;
  margin: 0 40rpx 0 10rpx;
}
.info .icon {
  margin-right: 12rpx;
}
.icon {
  display: block;
  width: 32rpx;
  height: 32rpx;
  background: no-repeat center;
  background-size: contain;
}
.icon-loc {
  background-image: url("https://cmspic-10004025.image.myqcloud.com/aa742fd0-dc47-11e8-a58f-eb006f54e2be_size_32x32");
}
.icon-tel {
  background-image: url("https://cmspic-10004025.image.myqcloud.com/8f122580-dc47-11e8-a58f-eb006f54e2be_size_32x32");
}
.icon-car {
  background-image: url("https://cmspic-10004025.image.myqcloud.com/594e1bc0-dc47-11e8-a58f-eb006f54e2be_size_32x32");
}

.icon-clock {
  background-image: url("https://cmspic-10004025.image.myqcloud.com/72caeb50-dc47-11e8-a58f-eb006f54e2be_size_32x32");
}
.more:after {
  content: "";
  display: block;
  border: 3rpx solid #ccc;
  border-width: 3rpx 3rpx 0 0;
  width: 16rpx;
  height: 16rpx;
  transform: rotate(45deg);
  margin-right: 30rpx;
}
.txt-blue {
  color: #5490ff;
}
.features {
  padding: 20rpx 20rpx 20rpx 0;
  border-top: 1px solid #e7e7e7;
  word-break: break-all;
}
.features view {
  display: inline-block;
  white-space: nowrap;
  margin-right: 40rpx;
  color: #666;
  font-size: 24rpx;
  line-height: 2em;
  vertical-align: middle;
}
.features view:before {
  content: "";
  display: inline-block;
  width: 24rpx;
  height: 24rpx;
  margin-right: 10rpx;
  background: url("data:image/png;base64,")
    no-repeat center;
  vertical-align: middle;
  background-size: contain;
}
.service .list {
  padding: 20rpx 0;
  overflow: hidden;
}
.service .list .item {
  display: inline-flex;
  flex-direction: column;
  align-items: center;
  width: 125rpx;
  overflow: hidden;
  margin: 10rpx 0;
}
.service .list .item image {
  width: 48rpx;
  height: 48rpx;
  margin-bottom: 5rpx;
}
.service .list .item .label {
  font-size: 20rpx;
  color: #666;
}
.pic .list {
  white-space: nowrap;
  overflow: auto;
  padding: 30rpx 5rpx 25rpx 25rpx;
}
.pic .list image {
  width: 160rpx;
  height: 160rpx;
  margin-right: 20rpx;
  border-radius: 8rpx;
}
