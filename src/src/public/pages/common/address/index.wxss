page {
  height: 100%;
  background: #fff;
}

.empty {
  border-top: 20rpx solid #efeff4;
  text-align: center;
  font-size: 28rpx;
  color: #666;
  background: #fff;
}

.empty image {
  display: block;
  width: 200rpx;
  height: 200rpx;
  margin: 120rpx auto 30rpx;
}

.button {
  display: flex;
  justify-content: space-around;
  margin-top: 60rpx;
}

.button button {
  width: 300rpx;
  height: 94rpx;
  margin: 61rpx auto;
  border-radius: 10rpx;
  line-height: 94rpx;
  color: #fff;
  font-size: 32rpx;
  background: #ff5390;
}

.button button:nth-of-type(1) {
  background: #ff5390;
}

.button button:nth-of-type(2) {
  background: #1aad19;
}

.addressList {
  height: 100%;
  background: #efeff4;
}

.address {
  width: 100%;
  height: 180rpx;
  background: #fff;
  border-top: 20rpx solid #efeff4;
}

.addressInfo {
  display: -webkit-box;
  display: -webkit-flex;
  display: -moz-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  -moz-box-pack: justify;
  -ms-flex-pack: justify;
  justify-content: space-between;
  -webkit-box-align: start;
  -webkit-align-items: flex-start;
  -moz-box-align: start;
  -ms-flex-align: start;
  align-items: flex-start;
  margin: 20rpx;
}

.addressInfoConcact {
  font-size: 32rpx;
  color: #000;
}

.addressInfoPhone {
  font-size: 32rpx;
  color: #121212;
}

.addressInfoDetail {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  flex: 8;
  padding-right: 20rpx;
  font-size: 28rpx;
  color: #666;
  line-height: 40rpx;
}

.edit {
  width: 30rpx;
  height: 26rpx;
  margin-top: 8rpx;
}


#addAddressDetail {
  height: 150rpx;
}

#defaultAddress {
  display: flex;
  justify-content: flex-end;
}

.save {
  width: 670rpx;
  height: 94rpx;
  margin: 61rpx auto;
  background: #FF5390;
  border-radius: 10rpx;
  font-size: 36rpx;
  color: #FFFFFF;
}
.toast{
  display: flex;
  justify-content:center;
  align-items: center;
  position: fixed;
  left:0;
  top:0;
  width:100%;
  height: 100%;
 
 
}
.toastContent{

  padding: 20rpx;
  border-radius: 20rpx;
  background: rgba(0,0,0,.5);
  text-align: center;
  color:#fff;
  font-size: 28rpx;
}
.deaf-txt {
  margin-right: 8rpx;
}
.rz-txt{
  display:inline-block;
  margin-left: 8rpx;
  padding: 5rpx 6rpx;
  background:#FF397E;
  color:#fff;
  font-size: 24rpx;
  font-weight:normal;
}